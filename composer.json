{
    "name": "umanit/seo-bundle",
    "description": "Provides SEO features on Doctrine entities.",
    "type": "symfony-bundle",
    "keywords": [
        "symfony",
        "doctrine",
        "SEO"
    ],
    "homepage": "https://github.com/umanit/seo-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "UmanIT",
            "email": "contact@umanit.fr"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "spatie/schema-org": "^2.1",
        "ramsey/uuid": "^4.1",
        "symfony/flex": "^1.9",
        "symfony/twig-bundle": "^4.4|5.1",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "doctrine/doctrine-bundle": "^1.12|^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "symfony/form": "^4.4|^5.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^5.1",
        "phpunit/phpunit": "^7.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Umanit\\SeoBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TestApp\\": "tests/Fixtures/src/",
            "Umanit\\SeoBundle\\Tests\\": "tests/"
        }
    }
}
